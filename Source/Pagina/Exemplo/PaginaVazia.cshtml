@page
@model PaginaVaziaModel
@{
    ViewData["Title"] = "Página Vazia";
}

<!-- Content Header (Page header) -->
<section class="content-header">
    <h1>
        Página vazia
        <small>descrição inicial da página</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> Inicio</a></li>
        <li><a href="#">Exemplos</a></li>
        <li class="active">Página vazia</li>
    </ol>
</section>

<!-- Main content -->
<section class="content">

